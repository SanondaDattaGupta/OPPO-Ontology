    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#HealthPurpose -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#HealthPurpose">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Purpose"/>
        <rdfs:label>Health purpose</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#HumanitarianPurpose -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#HumanitarianPurpose">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Purpose"/>
        <rdfs:label>Humanitarian purpose</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#IdentityData -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#IdentityData">
