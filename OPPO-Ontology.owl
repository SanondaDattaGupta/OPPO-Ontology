            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasStorageEntity"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#StorageEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasStorageLocation"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#SpatialEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Data storage location practice</rdfs:label>
        <skos:definition xml:lang="en">The data practices that discuss where and how the collected data is stored.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#DataStorageModificationPractice -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#DataStorageModificationPractice">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#DataStoragePrivacyPractice"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasResponseType"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#ResponseType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasResponseDelay"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#GeneralDurationDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Data storage modification practice</rdfs:label>
        <skos:definition xml:lang="en">The data practices that discuss how the data subject can modify (rectify/erase) the stored data.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#DataStoragePrivacyPractice -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#DataStoragePrivacyPractice">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#DataPractice"/>
        <rdfs:label xml:lang="en">Data storage practice.</rdfs:label>
        <skos:definition xml:lang="en">Data practices that discusses about the storage of the collected data.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#DataStorageRectificationPractice -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#DataStorageRectificationPractice">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#DataStorageModificationPractice"/>
        <rdfs:label xml:lang="en">Data storage rectification practice</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#DataSubject -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#DataSubject">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Role"/>
        <rdfs:label xml:lang="en">Data subject</rdfs:label>
        <skos:definition xml:lang="en">Data subject is defined as an individual from which an organization or OSN may collect data. In other word, Data subject is an individual who can identified directly or indirectly by the collected data. Note that a data subject can also be a data provider.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#DefiniteDurationDescription -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#DefiniteDurationDescription">
        <owl:equivalentClass>
            <owl:Class>
