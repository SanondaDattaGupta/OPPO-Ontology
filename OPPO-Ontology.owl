        <rdfs:label xml:lang="en">Legal agent</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#LegalPerson -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#LegalPerson">
