        <rdfs:label xml:lang="en">Data storage erasure practice</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#DataStorageLocationPractice -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#DataStorageLocationPractice">
