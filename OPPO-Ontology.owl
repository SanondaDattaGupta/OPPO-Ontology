            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2006/time#GeneralDurationDescription"/>
        <rdfs:label xml:lang="en">Definite duration description</rdfs:label>
    </owl:Class>
    


