        <rdfs:label xml:lang="en">Entity</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#FirstPartyDataRecipient -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#FirstPartyDataRecipient">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#DataRecipient"/>
        <rdfs:label>First party data recipient</rdfs:label>
        <skos:definition>If any data recipient recieves the data directly from a data subject, the data reipient is known as first party data recipient.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#GDPRFirstPartyDataRecipient -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#GDPRFirstPartyDataRecipient">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#FirstPartyDataRecipient"/>
        <rdfs:label>GDPR first party data recipient</rdfs:label>
        <skos:definition>GDPR first party data recipient are those first party data recipients whom the data subject directly provides their data and the data subject has the option to choose - opt out- so that the first party data recipient will not sell the collected data.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#GDPRThirdPartyDataRecipient -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#GDPRThirdPartyDataRecipient">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#ThirdPartyDataRecipient"/>
        <rdfs:label>GDPR third party data recipient</rdfs:label>
        <skos:definition>According to the privacy regulation GDPR, the third party data recipient is defined as follows: a natural or legal person, public authority, agency or body other than the data subject or first party data recipient and who are authorised to process personal data about the data subject.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#GeneticData -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#GeneticData">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#PersonalData"/>
