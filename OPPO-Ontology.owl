        <rdfs:label xml:lang="en">Legal person</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#MarketingPurpose -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#MarketingPurpose">
