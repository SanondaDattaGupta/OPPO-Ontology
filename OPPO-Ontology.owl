        <rdfs:label>Marketing purpose</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#Minor -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#Minor">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Role"/>
        <rdfs:label>Minor</rdfs:label>
        <skos:definition>According to privacy regulations, if any data subject is below a certain age limit (defined by the regulation), is considered as Minor and the organizations are reuiqred to follow special rules to process the data collected from Minor.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#OneFactorAuthenticationMechanism -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#OneFactorAuthenticationMechanism">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#AuthenticationMechanism"/>
        <rdfs:label xml:lang="en">One factor authentication mechanism</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#Organization -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#Organization">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#LegalAgent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasPolicy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://umaine.edu/Privacy/core/v1#PrivacyPolicy"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Organization</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#PasswordbasedAuthenticationMechanism -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#PasswordbasedAuthenticationMechanism">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#OneFactorAuthenticationMechanism"/>
        <rdfs:label xml:lang="en">Password-based authentication mechanism</rdfs:label>
    </owl:Class>
    



    


    <!-- http://umaine.edu/Privacy/core/v1#PersonalData -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#PersonalData">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Data"/>
        <rdfs:label xml:lang="en">Personal data</rdfs:label>
        <skos:definition xml:lang="en">Personal data is any piece of information that are raw and can be used to directly identify an individual. We defined seven categories of personal data and assigned them as sub classes of the personal data class.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#PersonalFinancialData -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#PersonalFinancialData">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#PersonalData"/>
        <rdfs:label xml:lang="en">Financial data</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#PrivacyPolicy -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#PrivacyPolicy">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Entity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasDatPractice"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#DataPractice"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Privacy policy</rdfs:label>
        <skos:definition xml:lang="en">A privacy policy is a legal contract between an organization and a privacy regulation that disclose the data practices of an organization in an attempt to comply with an specific privacy regulation.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#PrivacyRegulation -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#PrivacyRegulation">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Entity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#allows"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#DataPractice"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#disallows"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#DataPractice"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Privacy regulation</rdfs:label>
        <skos:definition xml:lang="en">A privacy regulation is law or set of rules that are required to be followed by any organization that collect, process, and share personal data about an individual.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#ProcessedData -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#ProcessedData">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Data"/>
        <rdfs:label xml:lang="en">Processed data</rdfs:label>
        <skos:definition xml:lang="en">Processed data is any piece of information that have been processed using different mechanism such as anonymous or pseuonymous so that the data cannot be used to directly identify an individual.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#ProfilingPurpose -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#ProfilingPurpose">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Purpose"/>
        <rdfs:label>Profiling purpose</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#PseudonymousProcessedData -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#PseudonymousProcessedData">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#ProcessedData"/>
        <rdfs:label xml:lang="en">Pseudonymous processed data</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#PublicInterestPurpose -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#PublicInterestPurpose">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Purpose"/>
        <rdfs:label>Public interest purpose</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#Purpose -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#Purpose">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Entity"/>
        <owl:disjointWith rdf:resource="http://umaine.edu/Privacy/core/v1#Role"/>
        <rdfs:label>Purpose</rdfs:label>
        <skos:definition>An organization may collect and process users&apos; personal information for several purposes such as health or marketting purpose. We reuse the purposes introduced by the PrOnto ontology and assigned them as the sub-classes for the purpose class.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#ResearchPurpose -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#ResearchPurpose">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Purpose"/>
        <rdfs:label>Research purpose</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#ResponseType -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#ResponseType">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Entity"/>
        <rdfs:label>Response type</rdfs:label>
        <skos:definition>An organization is required to response to any request placed by the data subject either by phone, mail, privacy policy, or electronic email.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#Role -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#Role">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Entity"/>
        <rdfs:label>Role</rdfs:label>
        <skos:definition>According to the privacy regulations such as GDPR or CCPA, the processing or collection of personal data vary based on different roles such as data provider and data recipient.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#SecurityMechanism -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#SecurityMechanism">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Entity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#appliesTo"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#Data"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Security mechanism</rdfs:label>
        <skos:definition xml:lang="en">According to the privacy regulations organization are required to mainatin proper security to protect the collected data-- often referred as  data safe guard mechanism.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#SecurityPurpose -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#SecurityPurpose">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Purpose"/>
        <rdfs:label>Security purpose</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#SocialAgent -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#SocialAgent">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Entity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#actsIn"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#Role"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Social agent</rdfs:label>
        <skos:definition xml:lang="en">An agentive physical object which is capable of actis as a social individual.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#SpatialEntity -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#SpatialEntity">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Entity"/>
        <rdfs:label xml:lang="en">Spatial entity</rdfs:label>
        <skos:definition xml:lang="en">Spatial location of storing personal data, such as Netharland, Finland.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#StatisticPurpose -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#StatisticPurpose">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Purpose"/>
        <rdfs:label>Statistic purpose</rdfs:label>
    </owl:Class>
    


