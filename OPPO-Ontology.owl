    <!-- http://umaine.edu/Privacy/core/v1#ActivityData -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#ActivityData">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#PersonalData"/>
        <rdfs:label xml:lang="en">Activity data</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#AnonymousProcessedData -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#AnonymousProcessedData">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#ProcessedData"/>
        <rdfs:label xml:lang="en">Anonymous processed data</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#AsymmetricEncryptionMechanism -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#AsymmetricEncryptionMechanism">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#EncryptionMechanism"/>
        <owl:disjointWith rdf:resource="http://umaine.edu/Privacy/core/v1#SymmetricEncryptionMechanism"/>
        <rdfs:label xml:lang="en">Asymmetric encryption mechanism</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#AuthenticationMechanism -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#AuthenticationMechanism">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#DataSafeGuardMechanism"/>
        <rdfs:label xml:lang="en">Authentication mechanism</rdfs:label>
        <skos:definition xml:lang="en">Authentication mechanism means ways to access collected data in an authorized manner.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#BiomentricData -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#BiomentricData">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#PersonalData"/>
        <rdfs:label xml:lang="en">Biometric data</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#BiometricbasedAuthenticationMechanism -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#BiometricbasedAuthenticationMechanism">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#OneFactorAuthenticationMechanism"/>
        <rdfs:label xml:lang="en">Biometric-based authentication mechanism</rdfs:label>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#CCPAFirstPartyDataRecipient -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#CCPAFirstPartyDataRecipient">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#FirstPartyDataRecipient"/>
        <rdfs:label>CCPA first party data recipient</rdfs:label>
        <skos:definition>According to the privacy regulation CCPA, a first party data recipient is any organization or entity that directly collects data from the data subject.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#CCPAThirdPartyDataRecipient -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#CCPAThirdPartyDataRecipient">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#ThirdPartyDataRecipient"/>
        <rdfs:label>CCPA third party data recipient</rdfs:label>
        <skos:definition>According to the privacy regulation CCPA, a third party can be any one (individual, organization, entity) except the service provider who directly collects personal information from the data subject.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#Data -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#Data">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Entity"/>
        <rdfs:label xml:lang="en">Data</rdfs:label>
        <skos:definition xml:lang="en">Data is any piece of information that can directly or indirectly identify an individual.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#DataPractice -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#DataPractice">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Entity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#actsOn"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#Data"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasAuthenticationMechanism"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#AuthenticationMechanism"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasBiometricbasedAuthenticationMechanism"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#BiometricbasedAuthenticationMechanism"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasEncryptionTechnique"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#EncryptionMechanism"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasPasswordbasedAuthenticationMechanism"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#PasswordbasedAuthenticationMechanism"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasPurpose"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#Purpose"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasSecurityMechanism"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#SecurityMechanism"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasTokenbasedAuthenticationMechanism"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#TokenbasedAuthenticationMechanism"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasTwoFactorAuthenticationMechanism"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#TwoFactorAuthenticationMechanism"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Data practice</rdfs:label>
        <skos:definition xml:lang="en">The process an organization follows to collect, process, share, or store the personal data.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#DataProvider -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#DataProvider">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Role"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#provides"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#Data"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Data provider</rdfs:label>
        <skos:definition xml:lang="en">Data provider defines as organizations or individuals which provide data to another organization(s) or individual(s).</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#DataRecipient -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#DataRecipient">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#Role"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#recieves"/>
                <owl:someValuesFrom rdf:resource="http://umaine.edu/Privacy/core/v1#Data"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Data provider</rdfs:label>
        <skos:definition>Data provider can be any organization or individual who provides data to the organization.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#DataSafeGuardMechanism -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#DataSafeGuardMechanism">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#SecurityMechanism"/>
        <rdfs:label xml:lang="en">Data safe guard mechanism</rdfs:label>
        <skos:definition xml:lang="en">Data safe guard mechanism may include any mechanism such as mechanisms to provide access to the collected data to the authorized  user, or encryption mechanism- a process of storing information in a code format.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#DataStorageDurationPractice -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#DataStorageDurationPractice">
        <rdfs:subClassOf rdf:resource="http://umaine.edu/Privacy/core/v1#DataStoragePrivacyPractice"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://umaine.edu/Privacy/core/v1#hasDurationPractice"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#GeneralDurationDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Data storage duration practice</rdfs:label>
        <skos:definition xml:lang="en">The data practices that discusses about the duration of the storage of personal data.</skos:definition>
    </owl:Class>
    


    <!-- http://umaine.edu/Privacy/core/v1#DataStorageErasurePractice -->

    <owl:Class rdf:about="http://umaine.edu/Privacy/core/v1#DataStorageErasurePractice">
